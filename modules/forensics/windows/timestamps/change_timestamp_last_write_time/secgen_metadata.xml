<?xml version="1.0"?>

<forensic xmlns="http://www.github/cliffe/SecGen/forensic"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.github/cliffe/SecGen/forensic">
  <name>Change timestamp last write time</name>
  <author>Jason Keighley</author>
  <module_license>Apache v2</module_license>
  <description>Change timestamp last write time</description>

  <type>timestamps</type>
  <platform>windows</platform>

  <!--optional details-->
  <reference></reference>

  <read_fact>last_write_time_file_path</read_fact>
  <read_fact>last_write_time_date</read_fact>

  <default_input into="last_write_time_file_path">
    <value>C:\Users\vagrant\Desktop\Hello.txt</value>
  </default_input>

  <default_input into="last_write_time_date">
    <value>11/25/2000 11:12:13</value>
  </default_input>

  <!--<requires>-->
    <!--<name>Create file</name>-->
  <!--</requires>-->

</forensic>